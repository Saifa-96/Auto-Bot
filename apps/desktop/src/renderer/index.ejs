<!doctype html>
<html>
  <head>
    <meta charset="utf-8" />
    <meta
      http-equiv="Content-Security-Policy"
      content="script-src 'self' 'unsafe-inline'"
    />
    <title>Auto Gui Bot</title>
  </head>
  <body>
    <div id="root"></div>
  </body>
</html>

<script>
  let pw = "";
  const open = (e) => {
    if (e.key === "Enter") {
      if (pw === "zxcvv") {
        window.debug.openDevTools();
        // window.removeEventListener("keydown", open);
      }
      pw = "";
      return;
    }
    pw += e.key;
  };
  window.addEventListener("keydown", open);
</script>
